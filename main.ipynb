   "source": [
    "import re\n",
    "\n",
    "# --- 1. Clean names (remove punctuation, normalize spaces) ---\n",
    "suffixes_pattern = r\"\\b(llc|inc|ltd|corp|gmbh|spa|sas|plc|co|company|limited)\\b\"\n",
    "\n",
    "\n",
    "def clean_name(name):\n",
    "    if isinstance(name, str):\n",
    "        name = name.lower()\n",
    "        name = re.sub(r\"[^\\w\\s]\", \"\", name)  # Remove punctuation\n",
    "        # name = re.sub(suffixes_pattern, '', name)   # Remove suffixes\n",
    "        name = re.sub(r\"\\s+\", \" \", name)  # Normalize spaces\n",
    "        return name.strip()\n",
    "    return \"\"\n",
    "\n",
    "\n",
    "# --- 2. Pre-clean both datasets ---\n",
    "df_companies_universe[\"SP_COMPANY_NAME_CLEAN\"] = df_companies_universe[\n",
    "    \"SP_COMPANY_NAME\"\n",
    "].map(clean_name)\n",
    "df_reprisk_id[\"company_name_clean\"] = df_reprisk_id[\"company_name\"].map(clean_name)\n",
    "\n",
    "# Need to drop duplicates because can be multiple rows of the same companies but different puntuation\n",
    "df_companies_universe = df_companies_universe.drop_duplicates(\n",
    "    subset=\"SP_COMPANY_NAME_CLEAN\", keep=\"first\"\n",
    ")\n",
    "df_reprisk_id = df_reprisk_id.drop_duplicates(subset=\"company_name_clean\", keep=\"first\")\n",
    "\n",
    "\n",
    "df_comp_repiq = df_companies_universe.merge(\n",
    "    df_reprisk_id,\n",
    "    left_on=\"SP_COMPANY_NAME_CLEAN\",\n",
    "    right_on=\"company_name_clean\",\n",
    "    how=\"inner\",\n",
    "    validate=\"one_to_one\",\n",
    ")\n",
    "\n",
    "df_comp_repiq.shape\n",
    "# left: (30895, 18)\n",
    "# right: (187452, 18)\n",
    "# inner: (4858, 18)\n",
    "# outer:  (213489, 18)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "2c26721f",
   "metadata": {},
   "outputs": [],
   "source": [
    "columns_to_drop = [\n",
    "    \"headquarters_country_isocode\",\n",
    "    \"url\",\n",
    "    \"isins\",\n",
    "    \"primary_isin\",\n",
    "    \"company_name_clean\",\n",
    "    \"headquarters_country\",\n",
    "    \"IQ_INDUSTRY_CLASSIFICATION\",\n",
    "    \"SP_COMPANY_NAME_CLEAN\",\n",
    "]\n",
    "\n",
    "df_comp_repiq = df_comp_repiq.drop(columns=columns_to_drop).rename(\n",
    "    columns={\"company_name\": \"company_name_reprisk\"}\n",
    ")"
   ]
  },
