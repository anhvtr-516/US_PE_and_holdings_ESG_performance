   "source": [
    "# Clean dates\n",
    "df_entry_exit[\"Entry_Date\"] = pd.to_datetime(\n",
    "    df_entry_exit[\"Entry_Date\"], errors=\"coerce\"\n",
    ")\n",
    "df_entry_exit[\"Exit_Date\"] = pd.to_datetime(df_entry_exit[\"Exit_Date\"], errors=\"coerce\")\n",
    "\n",
    "years = list(range(2007, 2021))\n",
    "\n",
    "# Initialize a list for results\n",
    "results = []\n",
    "\n",
    "# Group by Company\n",
    "for target_id, group in df_entry_exit.groupby(\"Target ID\"):\n",
    "    # Determine the latest date from Entry_Date or Exit_Date\n",
    "    group[\"Latest_Date\"] = group[[\"Entry_Date\", \"Exit_Date\"]].max(axis=1)\n",
    "    if group[\"Latest_Date\"].isna().all():\n",
    "        latest_row = group.iloc[-1,:]\n",
    "    else:\n",
    "        latest_row = group.loc[group[\"Latest_Date\"].idxmax()]\n",
    "       \n",
    "    company = latest_row[\"Company\"]\n",
    "    target = latest_row[\"Target\"]\n",
    "\n",
    "\n",
    "    # Track the PE firms for each year (list because there can be multiple PE owners)\n",
    "    backed_status = {year: [] for year in years}\n",
    "\n",
    "    for idx, row in group.iterrows():\n",
    "        entry = row[\"Entry_Date\"]\n",
    "        exit = row[\"Exit_Date\"]\n",
    "        pe_firm = row[\"PE_Firm\"]\n",
    "\n",
    "        # Handle missing entry date: assume ownership since start of observation\n",
    "        if pd.isna(entry):\n",
    "            entry = pd.Timestamp(\"2007-01-01\")\n",
    "\n",
    "        # Handle missing exit date: assume ownership until end of observation\n",
    "        if pd.isna(exit):\n",
    "            exit = pd.Timestamp(\"2020-12-31\")\n",
    "\n",
    "        # Skip if the exit was before 2007 (before observation window)\n",
    "        if exit.year < 2007:\n",
    "            continue\n",
    "\n",
    "        for year in years:\n",
    "            if entry.year <= year <= exit.year:\n",
    "                if pe_firm not in backed_status[year]:  # Avoid duplicates\n",
    "                    backed_status[year].append(pe_firm)\n",
    "\n",
    "    # Store the results\n",
    "    for year, firms in backed_status.items():\n",
    "        results.append(\n",
    "            {\n",
    "                \"Company\": company,\n",
    "                \"Target ID\": target_id,\n",
    "                \"Target\": target,\n",
    "                \"Year\": year,\n",
    "                \"PE_Firm\": firms,\n",
    "                \"PE_backed\": True if firms else False,\n",
    "            }\n",
    "        )\n",
    "\n",
    "# Create the final DataFrame\n",
    "df_PE_by_year = pd.DataFrame(results, copy=True)\n",
    "df_PE_by_year"
   ]
  },
