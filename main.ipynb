   "source": [
    "## Merge US companies universe with RepRisk Identifiers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "da5952be",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "pd.options.display.max_rows = 50\n",
    "\n",
    "# Read Excel, skip the first 3 rows, use the 4th row as header, drop any all-NaN rows\n",
    "df_companies_universe = (\n",
    "    pd.read_excel(\n",
    "        \"US Company Universe.xlsx\",\n",
    "        skiprows=3,  # skip rows 0, 1, 2\n",
    "        header=0,  # treat the next row (original row 3) as column names\n",
    "        dtype={\"SP_ENTITY_ID\": str},\n",
    "    )\n",
    "    .dropna(how=\"all\")\n",
    "    .reset_index(drop=True)\n",
    ")  # remove completely blank rows\n",
    "\n",
    "columns_to_keep = [\n",
    "    \"SP_ENTITY_NAME\",\n",
    "    \"SP_ENTITY_ID\",\n",
    "    \"SP_COMPANY_NAME\",\n",
    "    \"SP_COMPANY_STATUS\",\n",
    "    \"IQ_INDUSTRY_CLASSIFICATION\",\n",
    "    \"SP_COMPANY_TYPE\",\n",
    "    \"SPONSOR_BACKED_INFO\",\n",
    "]\n",
    "df_companies_universe = df_companies_universe[columns_to_keep].drop_duplicates(\n",
    "    subset=\"SP_COMPANY_NAME\", keep=False\n",
    ")"
   ]
  },
