   "source": [
    "rating_order = ['AAA', 'AA', 'A', 'BBB', 'BB', 'B', 'CCC', 'CC', 'C', 'D']\n",
    "\n",
    "def custom_mode(series):\n",
    "    if series.dropna().empty:\n",
    "        return None\n",
    "    \n",
    "    mode_values = series.mode()\n",
    "    \n",
    "    if mode_values.empty:\n",
    "        return None\n",
    "    \n",
    "    # If only one mode, return it\n",
    "    if len(mode_values) == 1:\n",
    "        return mode_values.iloc[0]\n",
    "    \n",
    "    # If multiple modes, return the one with the lowest rating (worst rating)\n",
    "    # according to the custom order\n",
    "    ranked = sorted(mode_values, key=lambda x: rating_order.index(x))\n",
    "    return ranked[-1]  # Worst rating has the highest index\n",
    "\n",
    "\n",
    "\n",
    "# Now group by RepRisk_ID and year\n",
    "df_reprisk_yearly = (\n",
    "    df_reprisk_index.groupby([\"RepRisk_ID\", \"year\"])\n",
    "    .agg(\n",
    "        {\n",
    "            \"current_RRI\": \"mean\", \n",
    "            \"RRI_trend\": \"sum\",  \n",
    "            \"peak_RRI\": \"max\",  \n",
    "            \"peak_RRI_date\": \"max\",  \n",
    "            \"RepRisk_rating\": custom_mode,\n",
    "            \"country_sector_average\": \"mean\", \n",
    "            \"environmental_percentage\": \"mean\", \n",
    "            \"social_percentage\": \"mean\",\n",
    "            \"governance_percentage\": \"mean\",\n",
    "            \"headquarter_country_code\": \"first\", \n",
    "            \"sectors\": \"first\",  \n",
    "        }\n",
    "    )\n",
    "    .reset_index()\n",
    ")"
   ]
